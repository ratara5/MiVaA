<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.actividadlabcorte2">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_banner_virus"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_banner_virus"
        android:supportsRtl="true"
        android:theme="@style/Theme.ActividadLabCorte2.NoActionBar">
        <activity
            android:name=".MainMenuActivity"
            android:label="@string/title_activity_main_menu"
            android:theme="@style/Theme.ActividadLabCorte2.NoActionBar"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".ui.jobService.servicio_api"
            android:permission="android.permission.BIND_JOB_SERVICE"/>
    </application>

</manifest>